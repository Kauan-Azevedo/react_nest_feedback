datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Feedback {
  id        Int      @id @default(autoincrement())
  title     String
  content   String
  name      String?
  CreatedAt DateTime @default(now())

  @@map("feedback")
}
