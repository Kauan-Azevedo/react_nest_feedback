datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "linux-musl"]
}

model Feedback {
  id        String   @id @default(uuid())
  title     String
  content   String
  name      String?
  CreatedAt DateTime @default(now())

  @@map("feedback")
}
